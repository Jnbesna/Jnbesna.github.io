          "media": {
            "image": {
              "active": true,
              "src": "@PROJECT_PATH@/assets/images/spr-research-1-797x565.jpg",
              "class": "mbr-figure__img",
              "width": 1400,
              "link": "",
              "alt": "",
              "title": ""
            },
            "video": {
              "class": "mbr-figure__img",
              "src": "http://www.youtube.com/embed/sOSTC6FM6Bw"
